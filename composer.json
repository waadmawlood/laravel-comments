{
    "name": "waad/laravel-comments",
    "description": "Add comments to your Laravel application",
    "keywords": [
        "beyondcode",
        "comments",
        "laravel-comments"
    ],
    "homepage": "https://github.com/beyondcode/laravel-comments",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
    },
    "autoload": {
        "psr-4": {
            "Waad\\Comments\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waad\\Comments\\CommentsServiceProvider"
            ]
        }
    }
}
